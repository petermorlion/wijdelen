@model IEnumerable<WijDelen.ObjectSharing.Models.ObjectRequestRecord>

@{
    Layout.Title = T("Your Requests");
}

<h1>@T("Your Requests")</h1>

<ul class="objectrequestlist">
    @foreach (var objectRequestRecord in Model) {
        <li class="objectrequestlist-objectrequest">
            @Html.ActionLink(objectRequestRecord.Description, "Item", new {area = "WijDelen.ObjectRequests", id = objectRequestRecord.AggregateId}, new { @class = "objectrequestlist-objectrequestlink" })
        </li>
    }
</ul>